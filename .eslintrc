{
    "env": {
    "browser": true,
    "node": true,
    "amd": true,
    "es6": true,
    "jasmine": true
    },
    "globals": {},
    "ecmaFeatures": {
    "modules": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "angular"
    ],
    "rules": {
        "angular/angularelement": 1,
        "angular/controller-as": 2,
        "angular/controller-as-route": 2,
        "angular/controller-as-vm": [2, "vm"],
        "angular/controller-name": [2, "/[A-Z].*Controller$/"],
        "angular/deferred": 0,
        "angular/definedundefined": 0,
        "angular/di": [0, "function"],
        "angular/di-order": [0, true],
        "angular/directive-name": 0,
        "angular/directive-restrict": [0, {"restrict": "AE", "explicit": "never"}],
        "angular/component-limit": [0, 1],
        "angular/document-service": 0,
        "angular/empty-controller": 0,
        "angular/file-name": 0,
        "angular/filter-name": 0,
        "angular/foreach": 0,
        "angular/function-type": 0,
        "angular/interval-service": 2,
        "angular/json-functions": 0,
        "angular/log": 2,
        "angular/module-dependency-order": [0, {"grouped": true, "prefix": null}],
        "angular/module-getter": 2,
        "angular/module-name": 0,
        "angular/module-setter": 2,
        "angular/no-angular-mock": 0,
        "angular/no-controller": 0,
        "angular/no-cookiestore": 2,
        "angular/no-http-callback": 2,
        "angular/no-inline-template": [2, {"allowSimple": true}],
        "angular/no-jquery-angularelement": 2,
        "angular/no-private-call": 2,
        "angular/no-run-logic": [0, {"allowParams": true}],
        "angular/no-service-method": 0,
        "angular/no-services": [2, ["$http", "$resource", "Restangular"]],
        "angular/on-watch": 2,
        "angular/one-dependency-per-line": 0,
        "angular/rest-service": 0,
        "angular/service-name": 2,
        "angular/timeout-service": 2,
        "angular/typecheck-array": 0,
        "angular/typecheck-date": 0,
        "angular/typecheck-function": 0,
        "angular/typecheck-number": 0,
        "angular/typecheck-object": 0,
        "angular/typecheck-regexp": 0,
        "angular/typecheck-string": 0,
        "angular/watchers-execution": [0, "$digest"],
        "angular/window-service": 0,
        "strict": 0,
        "no-console": 1,
        "dot-notation": [1, {
            "allowKeywords": false
        }],
        "wrap-iife": [1, "outside"],
        "brace-style": [1, "1tbs", {
            "allowSingleLine": true
        }],
        "comma-style": [1, "last"],
        "quotes": [1, "single", "avoid-escape"],
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": 1,
        "semi": [1, "always"],
        "max-len": [1, 120, 4],
        "one-var": 0,
        "vars-on-top": 0,
        "block-scoped-var": 0,
        "consistent-return": 1,
        "curly": [1, "all"],
        "eqeqeq": 1,
        "guard-for-in": 1,
        "no-caller": 1,
        "no-eval": 1,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-extra-semi": 1,
        "no-fallthrough": 1,
        "no-implied-eval": 1,
        "no-loop-func": 1,
        "no-native-reassign": 1,
        "no-new": 1,
        "no-new-func": 1,
        "no-new-wrappers": 1,
        "new-parens": 1,
        "no-proto": 1,
        "no-redeclare": 1,
        "no-return-assign": 1,
        "no-sequences": 1,
        "no-unused-expressions": 1,
        "yoda": 0,
        "no-underscore-dangle": 1,
        "eol-last": 0,
        "no-use-before-define": 0,
        "semi-spacing": 1,
        "camelcase": 1,
        "no-unused-vars": 1,
        "key-spacing": 1,
        "no-undef": 1,
        "no-shadow": 1,
        "no-multi-spaces": 1,
        "space-infix-ops": 1,
        "no-shadow-restricted-names": 1,
        "no-trailing-spaces": 1,
        "no-unreachable": 1,
        "new-cap": 1,
        "comma-spacing": 1,
        "space-unary-ops": 1,
        "no-empty": 1,
        "no-extra-parens": 1,
        "no-multi-str": 1,
        "no-spaced-func": 1,
        "no-extra-boolean-cast": 1,
        "no-cond-assign": 1,
        "no-func-assign": 1
    }
}
